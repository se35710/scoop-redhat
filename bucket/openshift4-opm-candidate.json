{
    "homepage": "https://www.openshift.com/",
    "description": "Red Hat OpenShift is a hybrid cloud, enterprise Kubernetes application platform, trusted by 2,000+ organizations. This manifest installs the OpenShift Operator Package Manager (OPM).",
    "license": "Apache-2.0",
    "version": "4.7.0-rc.3",
    "architecture": {
        "64bit": {
            "url": "https://mirror.openshift.com/pub/openshift-v4/clients/ocp/4.7.0-rc.3/opm-windows-4.7.0-rc.3.tar.gz",
            "hash": "50cb054dd09f264b78524a5dc23abbabaec5f325df42f5df5bcb94bd15aff015"
        }
    },
    "bin": "opm.exe",
    "installer": {
        "script": "Move-Item \"$dir\\windows-amd64-opm\" \"$dir\\opm.exe\""
    },
    "checkver": {
        "url": "https://mirror.openshift.com/pub/openshift-v4/clients/ocp/candidate/release.txt",
        "regex": "Name:\\s+((?<release>[\\d.-]+(rc.[\\d]+)))"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://mirror.openshift.com/pub/openshift-v4/clients/ocp/$version/opm-windows-$version.tar.gz"
            }
        },
        "hash": {
            "url": "$baseurl/sha256sum.txt"
        }
    }
}
