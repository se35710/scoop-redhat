    "license": "Apache-2.0",
    "version": "4.22.0-ec.2",
    "architecture": {
        "64bit": {
            "url": "https://mirror.openshift.com/pub/openshift-v4/clients/ocp-dev-preview/4.22.0-ec.2/openshift-client-windows-4.22.0-ec.2.zip",
            "hash": "sha256:placeholder"
        }
    },
    "bin": "oc.exe",
    "checkver": {
